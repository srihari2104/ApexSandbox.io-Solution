public List<Integer> positiveIntegers(List<Integer> numbers) {
    list<integer> posituiveNo = new list<integer>();
    for(n=0;n<numbers.size();n++){
            if(numbers[n]>0){
               //system.debug(numbers[n]);
              posituiveNo.add(numbers[n]);
            }
        }
        return posituiveNo;
}