public List<Integer> positiveIntegers(List<Integer> numbers) {
    list<integer> positiveNo = new list<integer>();

    for(integer n=0;n<numbers.size();n++){
            if(numbers[n]>0){
              //system.debug(numbers[n]);
              positiveNo.add(numbers[n]);
            }
        }
//system.debug(positiveNo);
        return positiveNo;
}