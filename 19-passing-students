public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    //code here
    Integer passedConditions = 0;

        if (passedExam) {
            passedConditions++;
        }
        
        if (passedAssignments) {
            passedConditions++;
        }
        
        if (passedProject) {
            passedConditions++;
        }
        
        // Check if at least two of the three conditions are true
        return passedConditions >= 2;
    }

}