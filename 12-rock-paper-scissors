public Integer rockPaperScissors(String player1, String player2) {
    List<String> s = new List<String>{'rock', 'paper', 'scissors'};

    Integer p1 = s.indexOf(player1);

    Integer p2 = s.indexOf(player2);

    if (p1 == p2) {

        return 0;

        } else if (p1 - p2 == 1 || (p1 == 0 && p2 == 2)) {

        return 1;

        } 

    return 2;
}